{
    "compilerOptions": {
        "declaration": true,
        "outDir": "./dist",
        "rootDirs": [
            ".",
        ],
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "strict": false,
        "types": ["@moddota/dota-lua-types/normalized"]
    },
    "include": [
        "./src/"
    ],
    "exclude": [],
    "tstl": {
        "luaTarget": "JIT",
        "sourceMapTraceback": true,
        "buildMode": "library"
    }
}
