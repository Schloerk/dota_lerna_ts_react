{
    "name": "custom_game_game",
    "description": "a ts-react template for dota2 modding, together with some helper scripts",
    "version": "1.0.0",
    "license": "MIT",
    "repository": "todo",
    "scripts": {
        "lint": "eslint --ext .js,.ts,.tsx --quiet --fix \"./game/scripts/src/**/*.{js,ts,tsx}\" \"./scripts/**/*.{js,ts,tsx}\" ",
        "prod": "tstl --project tsconfig.json",
        "dev": "tstl --project tsconfig.json --watch",
        "api": "openapi --input src/server/server_api.json --output src/server --exportCore false && node scripts/clean_api.js"
    },
    "devDependencies": {
        "@moddota/dota-lua-types": "^4.26.0",
        "@types/events": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^5.36.0",
        "@typescript-eslint/parser": "^5.36.0",
        "@typescript-to-lua/language-extensions": "^1.0.0",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "openapi-typescript-codegen": "^0.23.0",
        "prettier": "^2.7.1",
        "ts-loader": "^8.1.0",
        "ts-node": "^10.7.0",
        "typescript": "~4.9.3",
        "typescript-to-lua": "1.12.0",
        "custom_game_shared": "1.0.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
